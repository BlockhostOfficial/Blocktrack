{
  "name": "minetrack",
  "version": "5.6.1",
  "description": "A Minecraft server tracker that lets you focus on the basics.",
  "dependencies": {
    "finalhandler": "^1.1.2",
    "mcpe-ping-fixed": "^0.0.3",
    "mcping-js": "^1.5.0",
    "request": "2.88.2",
    "serve-static": "^1.14.1",
    "sqlite3": "^4.2.0",
    "uplot": "1.0.11",
    "winston": "^2.4.4",
    "ws": "^7.4.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cryptkeeper/Minetrack.git"
  },
  "keywords": [
    "minetrack"
  ],
  "author": "Nick Krecklow <hello@npm.nklow.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cryptkeeper/Minetrack/issues"
  },
  "homepage": "https://github.com/Cryptkeeper/Minetrack#README",
  "engines": {
    "node": ">=12.4.0"
  },
  "targets": {
    "main": {
      "engines": {
        "browsers": "defaults"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@types/finalhandler": "^1.1.1",
    "@types/mcping-js": "^1.5.1",
    "@types/serve-static": "^1.13.10",
    "@types/sqlite3": "^3.1.8",
    "@types/ws": "^8.2.2",
    "parcel": "^2.0.0-beta.2",
    "ts-node": "^10.4.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "eslint web/public/js/*.js && parcel build web/pages/index.html --dist-dir dist",
    "dev": "parcel build web/pages/index.html --no-optimize --dist-dir dist",
    "lint": "ts-standard",
    "fix": "ts-standard --fix",
    "run-server": "ts-node main.ts"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
